<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>StartServlet</servlet-name>
        <servlet-class>WebPart.servlet.StartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>WebPart.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>WebPart.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>


    <filter>
        <filter-name>RegisterFilter</filter-name>
        <filter-class>WebPart.Filter.RegisterFilter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>RegisterFilter</filter-name>
        <url-pattern>/registerFilter</url-pattern>
    </filter-mapping>

    <!--<filter>-->
        <!--<filter-name>autorizeFilter</filter-name>-->
        <!--<filter-class>WebPart.Filter.AutorizeFilter</filter-class>-->
    <!--</filter>-->
    <!--<filter-mapping>-->
        <!--<filter-name>autorizeFilter</filter-name>-->
        <!--<url-pattern>/</url-pattern>-->
    <!--</filter-mapping>-->

    <listener>
        <listener-class>
            WebPart.servlet.ConfigurationServletListener
        </listener-class>
    </listener>

</web-app>

